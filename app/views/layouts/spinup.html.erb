<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>


    <!-- General meta information -->
        <title>SpinUp Labs | <%= $title %></title>
        <meta name="keywords" content="<%= $keywords %>" />
        <meta name="description" content="<%= $description %>" />
        <meta name="robots" content="index, follow" />
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <!-- // General meta information -->


    <!-- Load stylesheets -->
        <!-- replace css/themes/xxxxxx.css with the theme you want to use -->
        <%= stylesheet_link_tag "clearlymodern/960", "clearlymodern/screen", "clearlymodern/themes/spin-up", "clearlymodern/jquery.fancybox-1.3.4" %>
        <!--[if lt IE 7]><link type="text/css" rel="stylesheet" href="clearlymodern/ie6.css" media="screen" /><![endif]-->
    <!-- // Load stylesheets -->


    <!-- Load Javascript / jQuery -->
        <!--[if lt IE 7]><script src="http://ie7-js.googlecode.com/svn/version/2.1(beta4)/IE7.js"></script><![endif]-->
        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"></script>
        <%= javascript_include_tag "clearlymodern/application" %>
    <!-- // Load Javascript / jQuery -->

    <!-- Favicon -->
        <link rel="shortcut icon" type="image/gif" href="/assets/favicon.gif" />
    <!-- // Favicon -->

</head>
<body>


	<div id="outer" class="clearfix">


        <div id="topbar" class="clearfix">

            <div class="container_12">

                <div class="grid_6">
                    <p><a href="/"><strong>Full Stack Web Development</strong></a></p>
                </div><!-- //grid_6 -->

                <div class="grid_6">
                    <p>phone: <strong>(866) 985-6877</strong> |  email: <strong><a href="#">info@spinuplabs.com</a></strong></p>
                </div><!-- //grid_4 -->


            </div><!-- //container_12 -->

        </div><!-- //topbar -->


		<div id="header" class="clearfix">

        	<div class="container_12">

           		<div class="grid_12" id="header-container">

                    <div id="logo">
                        <h1><a href="/" title="Go to the home page"><img src="/assets/spinup-logo-white-trans.png" alt="Logo" /></a></h1>
                    </div><!-- //logo -->

                    <ul class="sf-menu">
						<li><%= link_to 'Home','/' %></li>
            <li><%= link_to 'Why SpinUp','/spinup' %></li>
            <li><%= link_to 'Our Work','/work' %></li>
            <li><%= link_to 'Our Team','/team' %></li>
            <li><%= link_to 'Blog', '/blog/' %></li>
            <li><%= link_to 'Contact Us','/contact' %></li>
					</ul>
            	</div><!-- //grid_12 -->

			</div><!-- //container_12 -->


        </div><!-- //header -->


        <div id="main" class="clearfix">

              <%= yield %>


        </div><!-- //main -->




        <div id="footer" class="clearfix">
        	<div class="container_12">

                <div class="grid_6">

                    <div class="widget widget_links">
                    	<h3 class="widgettitle">Popular Blogs I Read</h3>
                        <ul>
                            <li><a href="#">Jason Cohen</a></li>
                            <li><a href="#">Eric Ries</a></li>
                            <li><a href="#">Brant Cooper</a></li>
                            <li><a href="#">Patrick Vlaskovits</a></li>
                            <li><a href="#">Brennan Dunn</a></li>

                        </ul>
                    </div>

                </div><!--//grid_6-->

                <div class="grid_6">

                    <div class="widget twitter-widget">
                    	<h3 class="widgettitle"><small class="right">Follow us <a href="http://twitter.com/#!/spinuplabs">@spinuplabs</a></small>Twitter</h3>
                           <div id="latest-footer-tweets"></div>
                    </div>

                </div><!--//grid_6-->

            </div><!--//container_12-->
        </div><!-- //footer -->

        <div id="footer-bottom" class="clearfix">
            <div class="container_12">
            	<div class="grid_12">
                    <p class="left">&copy; 2013 - SpinUp Labs </p>
                    <p class="right"><a href="#" class="scroll-top" title="Back to the Top">TOP</a></p>
                </div>
            </div><!--//container_12-->
        </div>


	</div><!-- //outer -->

	<!-- insert analytics -->
</body>
</html>
